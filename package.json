{
  "name": "fastify-lambda",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pm2 start process.json",
    "lambda": "npm run lambda:deploy",
    "lambda:deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1 --timeout 900",
    "lambda:update": "claudia update",
    "lambda:pack": "claudia pack",
    "lint": "semistandard | snazzy",
    "start": "NODE_ENV=dev nodemon -L -w ./src ./src/server.js",
    "test": "npm run lint && npm run test:unit",
    "test:coverage": "npm run test:unit -- --cov --coverage-report=text --coverage-report=html",
    "test:load": "artillery run ./test-load/root.v1.yml",
    "test:unit": "tap ./test/**/*.test.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "aws-lambda-fastify": "^1.3.0",
    "dotenv": "^8.2.0",
    "fastify": "^2.11.0",
    "fastify-autoload-recursive": "^1.0.0-rc.0",
    "fastify-compress": "^2.0.0",
    "fastify-cors": "^3.0.1",
    "fastify-gql": "^3.0.0",
    "fastify-helmet": "^3.0.2",
    "fastify-jwt": "^1.2.1",
    "fastify-mysql": "^1.0.0",
    "fastify-plugin": "^1.6.0",
    "fastify-redis": "^3.1.1",
    "fastify-static": "^2.5.1",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.8",
    "path": "^0.12.7",
    "sequelize": "^5.21.3",
    "swagger-ui-dist": "^3.25.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "artillery": "^1.6.0-29",
    "cheerio": "^1.0.0-rc.3",
    "claudia": "^5.12.0",
    "fastify-cli": "^1.3.0",
    "nock": "^11.7.2",
    "nodemon": "^2.0.2",
    "pino": "^5.16.0",
    "pm2": "^4.2.1",
    "pre-commit": "^1.2.2",
    "semistandard": "^14.2.0",
    "snazzy": "^8.0.0",
    "tap": "^14.10.6"
  }
}
