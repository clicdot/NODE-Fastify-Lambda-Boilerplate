{
  "name": "fastify-lambda",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pm2 start process.json",
    "lambda": "npm run lambda:deploy",
    "lambda:deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1 --timeout 900",
    "lambda:update": "claudia update",
    "lambda:pack": "claudia pack",
    "lint": "semistandard | snazzy",
    "start": "NODE_ENV=dev nodemon -L -w ./src ./src/server.js",
    "tap": "tap",
    "test": "npm run lint && npm run test:unit",
    "test:coverage": "npm run test:unit -- --cov --coverage-report=text --coverage-report=html",
    "test:load": "artillery run ./test-load/root.v1.yml",
    "test:unit": "tap ./test/**/*.test.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "aws-lambda-fastify": "^1.4.3",
    "dotenv": "^8.2.0",
    "fastify": "^2.14.1",
    "fastify-autoload-recursive": "^1.0.0-rc.0",
    "fastify-compress": "^2.0.1",
    "fastify-cors": "^3.0.3",
    "fastify-gql": "^3.8.0",
    "fastify-helmet": "^3.0.2",
    "fastify-jwt": "^1.3.1",
    "fastify-mysql": "^1.0.1",
    "fastify-plugin": "^1.6.1",
    "fastify-redis": "^3.3.1",
    "fastify-static": "^2.7.0",
    "fs-extra": "^9.0.0",
    "graphql": "^15.0.0",
    "path": "^0.12.7",
    "sequelize": "^5.21.9",
    "swagger-ui-dist": "^3.25.3",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "artillery": "^1.6.0",
    "cheerio": "^1.0.0-rc.3",
    "claudia": "^5.12.0",
    "eslint": "^7.0.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "fastify-cli": "^1.5.0",
    "nock": "^12.0.3",
    "nodemon": "^2.0.4",
    "pino": "^6.2.1",
    "pm2": "^4.4.0",
    "pre-commit": "^1.2.2",
